<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Student_Frisbee_Menu"
    android:background="@drawable/back5">

    <Button
        android:id="@+id/btn_frisbee_General"
        android:layout_width="200dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/borderbutton_black"
        android:onClick="frisbeeGeneral"
        android:text="General"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_frisbee_throws" />

    <Button
        android:id="@+id/btn_frisbee_throws"
        android:layout_width="200dp"
        android:layout_height="62dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/borderbutton_black"
        android:onClick="frisbeeThrows"
        android:text="Technique and throws"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>